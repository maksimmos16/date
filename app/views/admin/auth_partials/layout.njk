      //   $('.alert').slideUp('slow');
      //   var cells =  $('.alert');
      //   setTimeout(function(){
      //     cells.remove();
      //   },1000);
      // },5000);
    </script>
    {% block script %}{% endblock %}
  </body>
</html>

